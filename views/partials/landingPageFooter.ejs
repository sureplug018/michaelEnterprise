<script>
  document.addEventListener('DOMContentLoaded', function() {
    const tableContainer = document.querySelector('.table-responsive-xl');
    const indicator = document.createElement('div');
    indicator.className = 'scroll-indicator';
    indicator.textContent = 'Scroll →';

    // Append the indicator to the container
    if (tableContainer) {
      tableContainer.appendChild(indicator);

      // Auto-scroll the container
      tableContainer.scrollTo({
        left: 400, // Scroll to 200px initially (adjust as needed)
        behavior: 'smooth',
      });

      // Hide the indicator after 3 seconds
      setTimeout(() => {
        indicator.style.display = 'none';
      }, 3000);
    }

    // Remove the indicator if the user scrolls manually
    tableContainer.addEventListener('scroll', () => {
      indicator.style.display = 'none';
    });
  });
</script>

<script>
  function toggleProteins(event) {
    const button = event.target;
    const proteinsList = button.nextElementSibling;

    // Toggle visibility
    if (proteinsList.style.display === 'none') {
      proteinsList.style.display = 'block';
      button.textContent = 'Hide Proteins';
    } else {
      proteinsList.style.display = 'none';
      button.textContent = 'View Proteins';
    }
  }
</script>

