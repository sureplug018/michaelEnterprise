            <div class="tab-pane fade show active" id="pills-grid" role="tabpanel" aria-labelledby="pills-grid-tab" tabindex="0">
              <div class="dishes-card-wrap style2">
                <% products.forEach(function(product){ %>
                <div class="dishes-card style2 wow fadeInUp" data-wow-delay="0.2s">
                  <div class="dishes-thumb">
                    <a href="/michael-kitchen/item/<%= product.categorySlug %>/<%= product.slug %>">
                      <img src="<%= product.imageCover %>" style="width: 160px; height: 160px; border-radius: 50%; object-fit: cover;" alt="thumb">
                      <div class="circle-shape"><img class="cir36" src="/assets/img/food-items/circleShape.png" alt="shape"></div>
                    </a>
                  </div>
                  <div class="dishes-content">
                    <a href="/michael-kitchen/item/<%= product.categorySlug %>/<%= product.slug %>">
                      <h3><%= product.name %></h3>
                    </a>
                    <div class="star"><img src="https://gramentheme.com/html/fresheat/assets/img/icon/star2.svg" alt="icon"></div>
